# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Example' do
    # Ignore all warnings from all pods
  inhibit_all_warnings!
  
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Data Base
  pod 'RealmSwift'
  
  # Keyboard
  pod 'IQKeyboardManagerSwift'
  
  # Text Field
  pod 'TweeTextField'
  
  # Animation
  pod 'Hero'
  
  # Alert
  pod 'CDAlertView'
  
  # Facebook
  pod 'FacebookCore'
  pod 'FacebookLogin'
  pod 'FacebookShare'
  
  # HUD
  pod 'SVProgressHUD'
  
  pod 'Nantes'
  pod 'SwiftDate'
    
  # Google Maps
  pod 'GoogleMaps'
  pod 'GooglePlaces'

  pod 'Firebase/Analytics'
  pod 'Firebase/Auth'
  pod 'Firebase/Core'
  pod 'Firebase/Firestore'

  pod 'KeychainAccess'
  
  pod 'R.swift'

end
